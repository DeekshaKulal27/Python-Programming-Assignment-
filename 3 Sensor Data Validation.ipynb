   "source": [
    "sensor_readings = [3, 4, 7, 8, 10, 12, 5]\n",
    "\n",
    "valid_readings = []\n",
    "\n",
    "for hour, value in enumerate(sensor_readings):\n",
    "    if value % 2 == 0:   # Check if even\n",
    "        valid_readings.append((hour, value))\n",
    "\n",
    "print(\"Valid Sensor Readings (Hour, Value):\")\n",
    "print(valid_readings)\n"
   ]
